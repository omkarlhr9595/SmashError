// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

generator client {
  provider = "prisma-client-js"
}

//npx prisma migrate dev --name init
//npx prisma generate


model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}
